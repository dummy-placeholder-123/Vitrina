{
  "family": "vitrina-mp-task-def",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::289956717103:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "alpha-vitrina-mp",
      "image": "REPLACE_ME",
      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "name": "alpha-vitrina-mp-80-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "DB_HOST",
          "value": "vitrina-prod.ckb20k4wmdgc.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_NAME",
          "value": "vitrina-prod"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/vitrina-mp-task-def",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "secrets": [
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:289956717103:secret:rds!db-92458258-0d92-48ff-812b-e991e9fdcaaa-SUaHuK:username::"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:289956717103:secret:rds!db-92458258-0d92-48ff-812b-e991e9fdcaaa-SUaHuK:password::"
        }
      ]
    }
  ],
  "volumes": [],
  "placementConstraints": []
}
